{
  "name": "ide-vala",
  "main": "./lib/main",
  "version": "0.2.0",
  "description": "Vala language support for Atom-IDE",
  "repository": "https://github.com/naaando/ide-vala",
  "license": "GPL",
  "keywords": [
    "atom-ide",
    "vala",
    "lsp",
    "language-client"
  ],
  "scripts": {
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "eslint": "^6.1.0",
    "eslint-config-prettier": "^6.12.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "standard-version": "^9.0.0"
  },
  "husky": {
    "hooks": {
      "precommit": "npm run format",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "enhancedScopes": [
    "source.vala"
  ],
  "configSchema": {
    "vlsPath": {
      "type": "string",
      "title": "Vala language server path",
      "default": "vala-language-server",
      "description": "Use custom Vala language server path"
    }
  },
  "engines": {
    "atom": ">=1.21.0"
  },
  "package-deps": [
    "language-vala-modern",
    "atom-ide-ui"
  ],
  "dependencies": {
    "atom-languageclient": "0.8.1",
    "atom-package-deps": "^4.6.0"
  },
  "consumedServices": {
    "linter-indie": {
      "versions": {
        "2.0.0": "consumeLinterV2"
      }
    }
  },
  "providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provideAutocomplete"
      }
    }
  }
}
